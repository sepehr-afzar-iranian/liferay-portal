<liferay-ui:search-container-column-text
	href="<%= rowURL %>"
	name="user-id"
	property="userId"
/>

<liferay-ui:search-container-column-text
	href="<%= rowURL %>"
	name="user-name"
	property="userName"
/>

<liferay-ui:search-container-column-text
	href="<%= rowURL %>"
	name="resource-id"
	property="classPK"
/>

<%
String resourceName = event.getClassName();
%>

<liferay-ui:search-container-column-text
	href="<%= rowURL %>"
	name="resource-name"
	value="<%= Validator.isNotNull(resourceName)?resourceName.substring(resourceName.lastIndexOf('.') + 1):null %>"
/>

<liferay-ui:search-container-column-text
	href="<%= rowURL %>"
	name="resource-action"
	value="<%= event.getEventType() %>"
/>

<liferay-ui:search-container-column-text
	href="<%= rowURL %>"
	name="client-ip"
	value="<%= Validator.isNotNull(event.getClientIP()) ? event.getClientIP() : StringPool.BLANK %>"
/>

<liferay-ui:search-container-column-date
	href="<%= rowURL %>"
	name="create-date"
	value="<%= event.getCreateDate() %>"
/>